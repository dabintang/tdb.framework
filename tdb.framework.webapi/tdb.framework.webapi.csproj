<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.1</TargetFramework>
    <Version>2.1.0.10</Version>
    <Authors>tangdabin</Authors>
    <Company>tdb</Company>
    <Description>webapi框架</Description>
    <PackageReleaseNotes>webapi框架</PackageReleaseNotes>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <PackageProjectUrl>https://github.com/dabintang/tdb.framework</PackageProjectUrl>
    <PackageTags>tdb</PackageTags>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Autofac.Extensions.DependencyInjection" Version="7.1.0" />
    <PackageReference Include="Swashbuckle.AspNetCore" Version="6.1.0" />
    <PackageReference Include="tdb.appsettings" Version="2.1.0.1" />
    <PackageReference Include="tdb.consul.kv" Version="2.1.0.3" />
    <PackageReference Include="tdb.csredis" Version="2.1.0.2" />
    <PackageReference Include="tdb.jwt" Version="2.1.0.5" />
    <PackageReference Include="tdb.nlog.mysql" Version="2.1.0.4" />
  </ItemGroup>
  
  <PropertyGroup>
    <GenerateDocumentationFile>true</GenerateDocumentationFile>
  </PropertyGroup>

</Project>
